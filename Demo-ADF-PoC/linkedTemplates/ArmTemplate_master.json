{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Demo-ADF-PoC"
		},
		"AzureBlobStorageForADF_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageForADF'"
		},
		"AzureBlobStorageOnPrem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageOnPrem'"
		},
		"PostgreSqlOnPrem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PostgreSqlOnPrem'"
		},
		"SqlServerOnPrem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerOnPrem'"
		},
		"SqlServerOnPrem1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerOnPrem1'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Demo-ADF-PoC_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageForADF_connectionString": {
						"value": "[parameters('AzureBlobStorageForADF_connectionString')]"
					},
					"AzureBlobStorageOnPrem_connectionString": {
						"value": "[parameters('AzureBlobStorageOnPrem_connectionString')]"
					},
					"PostgreSqlOnPrem_connectionString": {
						"value": "[parameters('PostgreSqlOnPrem_connectionString')]"
					},
					"SqlServerOnPrem_connectionString": {
						"value": "[parameters('SqlServerOnPrem_connectionString')]"
					},
					"SqlServerOnPrem1_connectionString": {
						"value": "[parameters('SqlServerOnPrem1_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}